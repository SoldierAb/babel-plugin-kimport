{
  "name": "babel-plugin-kimport",
  "version": "1.0.2",
  "description": "A component modular import plugin for babel ",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib",
    "lint": "eslint --ext .js src --fix",
    "release": "standard-version",
    "commit": "git add . && git-cz",
    "test": "mocha --require @babel/register",
    "debug": "mocha --require @babel/register --require @babel/polyfill --no-timeouts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verify-commit.js"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SoldierAb/babel-plugin-kimport.git"
  },
  "keywords": [
    "babel-plugin",
    "import"
  ],
  "author": "cgj",
  "license": "MIT",
  "homepage": "https://github.com/SoldierAb/babel-plugin-kimport#readme",
  "bugs": {
    "url": "https://github.com/SoldierAb/babel-plugin-kimport/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/helper-module-imports": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.7.7",
    "@babel/types": "^7.7.4",
    "assert": "^2.0.0",
    "babel-eslint": "^10.0.3",
    "babylon": "^6.18.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "git-cz": "^3.3.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^1.3.0",
    "lint-staged": "^9.5.0",
    "mocha": "^7.0.0",
    "pre-commit": "^1.2.2",
    "standard-version": "^7.0.1",
    "yorkie": "^2.0.0"
  }
}
